{
  "name": "data-extractor-monorepo",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "echo 'Use package-level dev scripts'",
    "dev:all": "bash scripts/dev.sh up",
    "stop:all": "bash scripts/dev.sh down",
    "restart:all": "bash scripts/dev.sh restart",
    "status:all": "bash scripts/dev.sh status",
    "compose:up": "docker compose -f infrastructure/docker-compose.yml up --build",
    "compose:down": "docker compose -f infrastructure/docker-compose.yml down -v",
    "lint": "echo 'lint placeholder'",
    "test": "echo 'test placeholder'"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
